{
  "ConnectionStrings": {
    "DefaultConnection": "Server=LT-VAR-T-MY\\SQLEXPRESS;Database=StockDemo;MultipleActiveResultSets=true;Integrated Security=True"
  },
  "SwaggerConfig": {
    "JsonRoute": "swagger/{documentName}/swagger.json",
